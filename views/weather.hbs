<!DOCTYPE html>
<html lang="en">
<head>
    {{>links}}
</head>
<body>
   {{>header}}
   <!-- main header -->
   <div class="container-fluid main_header" >
    <div class="row">
        <div class="col-md-10 col-12 mx-auto" >
            <div class="main_content">
                <form  class="temp_form" data-aos="fade-right">
<input type="text" id="cityName" placeholder="Enter Your City Here" autocomplete="off">
            <input type="submit" value="search" id="submitBtn">
                </form>
            </div>
            <div class="tempInformation" data-aos="flip-up">
                <div class="top_layer">
                    <p id="day"></p>
                    <p id="today_data"></p>
                </div>
                <div class="main_layer">
                    <p id="city_name">Get Output Here</p>
                    <div class="middle_layer data_hide">
                        <p id="temp"><span id="tempp">0</span><sup>o</sup>C</p>
                        <p id="temp_status"></p>
                    </div>
                </div>
            </div>
        </div>
     </div>
   </div>
{{>footer}}
<script>
  AOS.init();
  </script>
  <script src="js/main.js"></script>
</body>
</html>